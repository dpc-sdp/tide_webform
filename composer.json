{
    "name": "dpc-sdp/tide_webform",
    "description": "Webform configuration for Tide Drupal 8 distribution",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "require": {
        "drupal/config_filter": "1.*",
        "drupal/config_ignore": "^2.1",
        "drupal/token_conditions": "dev-1.x",
        "drupal/webform": "5.0.0-rc18",
        "dpc-sdp/tide_core": "dev-feature/SDPA-1704_event-log-failed-password"
    },
    "suggest": {
        "dpc-sdp/tide_landing_page": "@dev",
        "dpc-sdp/tide_api": "@dev",
        "dpc-sdp/tide_page": "@dev"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/webform": {
                "Error processing Webform Tokens, sending leaked metadata - https://www.drupal.org/project/webform/issues/3010727#comment-12838354": "https://www.drupal.org/files/issues/2018-11-01/3010727-4.patch"
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.1"
        },
        "process-timeout": 0,
        "sort-packages": true
    }
}
